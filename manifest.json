{
    "manifest_version": 2,
    "name": "Tab Time Tracker",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  